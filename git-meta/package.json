{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "git-meta",
  "description": "Identify files that belong to deleted git-meta submodules, and exclude them from search results",
  "activationEvents": [
    "*"
  ],
  "wip": true,
  "categories": [],
  "tags": [],
  "contributes": {
    "actions": [],
    "menus": {
      "editor/title": [],
      "commandPalette": []
    },
    "configuration": {}
  },
  "version": "0.0.0-DEVELOPMENT",
  "license": "MIT",
  "main": "dist/git-meta.js",
  "scripts": {
    "tslint": "tslint -p tsconfig.json './src/**/*.ts'",
    "typecheck": "tsc -p tsconfig.json",
    "prettier": "prettier '**/{*.{js?(on),ts?(x),scss,yml},.*.js?(on),.*.yml}' --write --list-different",
    "prettier-check": "npm run prettier -- --write=false",
    "build": "parcel build --out-file dist/git-meta.js src/git-meta.ts",
    "symlink-package": "mkdirp dist && lnfs ./package.json ./dist/package.json",
    "serve": "npm run symlink-package && parcel serve --no-hmr --out-file dist/git-meta.js src/git-meta.ts",
    "watch:typecheck": "tsc -p tsconfig.json -w",
    "watch:build": "tsc -p tsconfig.dist.json -w",
    "sourcegraph:prepublish": "npm run build"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Edge versions",
    "last 1 Safari versions"
  ],
  "devDependencies": {
    "@sourcegraph/prettierrc": "^3.0.1",
    "@sourcegraph/tsconfig": "^4.0.0",
    "@sourcegraph/tslint-config": "^13.4.0",
    "lnfs-cli": "^2.1.0",
    "mkdirp": "^0.5.1",
    "parcel-bundler": "^1.12.4",
    "rxjs": "^6.5.3",
    "sourcegraph": "^23.0.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  }
}
